<VBox alignment="CENTER" prefHeight="559.0" prefWidth="782.0" style="-fx-background-color: #1B1D29;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="code.DeleteAccountController">
   <children>
      <Label text="Delete Account" textFill="#08bdff">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <ScrollPane hbarPolicy="NEVER" prefHeight="417.0" prefWidth="782.0" style="-fx-background-color: #2D3142;">
         <content>
            <GridPane fx:id="list" alignment="TOP_CENTER" prefHeight="415.0" prefWidth="768.0" style="-fx-background-color: #2D3142;">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
            </GridPane>
         </content>
      </ScrollPane>
      <HBox alignment="CENTER" prefHeight="61.0" prefWidth="782.0" spacing="50.0">
         <children>
            <Button fx:id="deleteBtn" mnemonicParsing="false" style="-fx-background-color: #2D3142;" text="Delete" textFill="#08bdff" />
            <Button fx:id="clearBtn" mnemonicParsing="false" style="-fx-background-color: #2D3142;" text="Clear" textFill="#08bdff" />
         </children>
      </HBox>
   </children>
</VBox>
